<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.web.WebView?>

<BorderPane prefHeight="303.0" prefWidth="405.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.WebBrowserController">
    <center>
        <WebView fx:id="webView" />
    </center>
   <top>
      <HBox prefHeight="32.0" prefWidth="800.0" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="addressBar" prefHeight="32.0" prefWidth="351.0" promptText="https://google.com" HBox.hgrow="ALWAYS" />
            <Button fx:id="loadButton" mnemonicParsing="false" onAction="#loadURL" prefHeight="31.0" prefWidth="42.0" text="Load" />
         </children>
      </HBox>
   </top>
</BorderPane>
